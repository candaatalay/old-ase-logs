x = 0:1:40;
tiledlayout(2,1);
 
nexttile
%Reaction E found from moment E
D = (20833.3-1666.6*x);
plot(x,D, 'r')
xlabel('x moving in between 0 and 40'), 
ylabel('Reaction D (Newton)'), 
title('Reaction force from point D wrt point X'), 
 
nexttile
%Reaction D found from moment D
E = (8150+1666.6*x);
plot(x,E, 'b')
xlabel('x moving in between 0 and 40'), 
ylabel('Reaction E (Newton)'),  
title('Reaction force from point D wrt point X'), 
